{
  "name": "qti-pci-bundler",
  "version": "2.1.1",
  "description": "Citolab custom pci builder for IMS pcis and TAO",
  "keywords": [
    "QTI",
    "render",
    "item",
    "IMS Global",
    "TAO",
    "PCI",
    "React",
    "Preact",
    "Tailwind"
  ],
  "homepage": "https://github.com/Citolab",
  "bugs": {
    "url": "https://github.com/Citolab/qti-pci-bundler/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Citolab/qti-pci-bundler"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "publish-packages": "lerna publish --force-publish",
    "publish-packages-next": "lerna publish prerelease --dist-tag next --force-publish --preid beta",
    "diff": "lerna diff",
    "list": "lerna list",
    "changed": "lerna changed",
    "test:init:local": "npm --prefix lib/tspci run test:init:local",
    "test:init:smoke": "npm --prefix lib/tspci run test:init:smoke",
    "test:init:matrix": "npm --prefix lib/tspci run test:init:matrix"
  },
  "workspaces": [
    "lib/preact-store",
    "lib/tspci",
    "lib/tspci-qbci",
    "lib/tspci-qti3",
    "lib/tspci-tao",
    "pcis/**/*"
  ],
  "contributors": [
    {
      "name": "Patrick de Klein",
      "email": "patrick.deklein@cito.nl"
    },
    {
      "name": "Marcel Hoekstra",
      "email": "marcel.hoekstra@cito.nl"
    },
    {
      "name": "Tjeerd Hans Terpstra",
      "email": "tjeerdhans.terpstra@cito.nl"
    },
    {
      "name": "Sander Looise",
      "email": "sander.looise@cito.nl"
    }
  ],
  "devDependencies": {
    "lerna": "^9.0.3"
  },
  "overrides": {
    "tar": "^6.2.0"
  }
}
