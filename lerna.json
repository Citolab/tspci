{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "version": "2.8.3",
  "npmClient": "npm",
  "command": {
    "publish": {
      "ignoreChanges": [
        "*.md"
      ],
      "message": "chore(release): publish",
      "registry": "https://registry.npmjs.org/",
      "access": "public",
      "yes": true,
      "conventionalCommits": true,
      "distTag": "latest",
      "version": "patch"
    }
  }
}